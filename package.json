{
  "name": "dialute-hook",
  "description": "Dialute Starter Svelte - Hook part",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc && copyfiles -u -a ../app/public/** ./**",
    "dev": "nodemon --exec npx ts-node src/index.ts",
    "lint": "eslint .",
    "format": "prettier --write ./src/**/*.{js,json,html,ts}"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "dialute": "^0.0.29",
    "node-fetch": "^3.2.5"
	"node": "^16.14.0"
  }
}
